# Frontend production environment variables
REACT_APP_API_URL=https://growthcanvas.xyz
REACT_APP_ENV=production

# OAuth redirect URLs (production)
REACT_APP_GOOGLE_OAUTH_REDIRECT_URL=https://growthcanvas.xyz/api/auth/callback/google
REACT_APP_GITHUB_OAUTH_REDIRECT_URL=https://growthcanvas.xyz/api/auth/callback/github
REACT_APP_LINKEDIN_OAUTH_REDIRECT_URL=https://api.ryze.ai/api/auth/callback/linkedin

# Feature flags
REACT_APP_ENABLE_OAUTH=true
REACT_APP_ENABLE_REGISTRATION=true

# Analytics and monitoring (if applicable)
REACT_APP_ENABLE_ANALYTICS=true